<idea-plugin>
    <id>com.primer.efp_plugin</id>
    <name>EfpHelper</name>
    <vendor>EfpDever</vendor>
    <description>IDEA plug-in for highly customized Efp projects.Provide configuration generation, code generation and
        other functions, greatly improve the development efficiency
    </description>
    <!-- please see http://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
         on how to target different products -->
    <depends>com.intellij.modules.platform</depends>
    <depends>com.intellij.modules.lang</depends>
    <depends>com.intellij.modules.java</depends>
    <depends>com.intellij.modules.xml</depends>
    <depends>com.intellij.properties</depends>
    <depends config-file="" optional="true">com.intellij.modules.database</depends>
    <depends>com.intellij.modules.vcs</depends>
    <idea-version since-build="183.0"/>
    <extensions defaultExtensionNs="com.intellij">
        <!-- Add your extensions here -->
        <!--dubbo服务调用-->
        <intentionAction>
            <className>com.efp.plugins.dubbo.action.DubboServiceCall</className>
            <category>Java/Annotations</category>
            <descriptionDirectoryName>MakeAnnotationExternal</descriptionDirectoryName>
        </intentionAction>
        <intentionAction>
            <className>com.efp.plugins.dubbo.action.DubboConfigCall</className>
            <category>Java/Annotations</category>
            <descriptionDirectoryName>MakeAnnotationExternal</descriptionDirectoryName>
        </intentionAction>

        <!--<applicationConfigurable groupId="Other Settings" id="efp.intellij.plugin.base" displayName="Efp Intellij Plugin Settings"
                instance="com.efp.plugins.settings.EfpCommonConfigurable">
                </applicationConfigurable>-->

    </extensions>

    <actions><!--AllIcons.Actions.Back-->

        <!--toolmenu配置-->
        <group id="efp_helper_dvk_tools_menu" text="Efp Little Helper" popup="true" >
            <add-to-group group-id="ToolsMenu" anchor="last"/>
        </group>
<!--

        &lt;!&ndash;dubboxml配置生成&ndash;&gt;
        <action id="dubbo-xml-config" class="com.efp.plugins.dubbo.action.DubboXmlConfigAction"
                text="EFP: Generate Dubbo Config">
            &lt;!&ndash;<add-to-group group-id="EditorPopupMenu" anchor="first"></add-to-group>&ndash;&gt;
            <add-to-group group-id="efp_helper_dvk_tools_menu" anchor="first"/>
        </action>
-->

        <!--dabatase插件配置-->
        <group id="efp_database_plugin" text="Efp Database Plugin" popup="true" >
            <add-to-group group-id="DatabaseViewPopupMenu" anchor="last"/>
        </group>
        <!--生成curd代码-->
        <action id="efp-plugin-code-helper" description="Generate Crud Code" text="Generate Crud Code"
                class="com.efp.plugins.codeHelper.action.CodeHelperAction">
            <add-to-group anchor="last" group-id="efp_database_plugin"/>
        </action>
        <!--生成insertOrUpdate相关代码-->
        <action id="efp-plugin-Generate-InsertOrUpdate" description="Generate InsertOrUpdate Method Code"
                text="Generate InsertOrUpdate Method Code"
                class="com.efp.plugins.codeHelper.action.GenerateInsertOrUpdate">
            <add-to-group anchor="last" group-id="efp_database_plugin"/>
        </action>
        <action id="efp-plugin-Generate-Select" description="Generate Select Method Code"
                text="Generate Select Method Code"
                class="com.efp.plugins.codeHelper.action.GenerateSelect">
            <add-to-group anchor="last" group-id="efp_database_plugin"/>
        </action>

        <!--直接对当前的dubbo服务发起调用-->
        <action id="efp-plugin-DubboRemoteInvoke" class="com.efp.plugins.dubbo.action.DubboRemoteInvoke"
                text="EFP: Call Current Dubbo Method">
            <add-to-group group-id="GenerateGroup" anchor="first"/>
        </action>
        <!--        <action id="efp-plugin-DiffCompare" class="com.efp.plugins.diffcompare.action.DiffCompareAction" text="DiffCompare">
                    <add-to-group group-id="efp_helper_dvk_tools_menu" anchor="first"/>
                </action>-->
        <!--        <action id="EFP-test" class="com.efp.plugins.test.testAction" text="EFP: test">
                    <add-to-group group-id="efp_helper_dvk_tools_menu" anchor="first"/>
                </action>-->

       <!--标签管理-->
        <action id="efp-plugin-ExportBookMarkAction" class="com.efp.plugins.bookmark.action.ExportBookMarkAction"
                text="Export BookMark To File">
            <add-to-group group-id="efp_helper_dvk_tools_menu"/>
        </action>
        <action id="efp-plugin-ImportBookMarkAction" class="com.efp.plugins.bookmark.action.ImportBookMarkAction"
                text="Import BookMark From File">
            <add-to-group group-id="efp_helper_dvk_tools_menu"/>
        </action>
    </actions>
</idea-plugin>